<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <IdeaActionID>Git.Add</IdeaActionID>
          <ShortCut>'Ctrl+Alt+A'</ShortCut>
          <Description>Add</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Build">
        <Item>
          <IdeaActionID>ActivateBuildToolWindow</IdeaActionID>
          <ShortCut>'Alt+0'</ShortCut>
          <Description>Build</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Development'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Alt+F5'</ShortCut>
          <Description>Debug 'Development'</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F12' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+Insert'</ShortCut>
          <Description>New Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh Stack Frame">
        <Item>
          <IdeaActionID>RefreshStackFrame</IdeaActionID>
          <ShortCut>'Button1 Double-Click'</ShortCut>
          <Description>Refresh Stack Frame</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Development'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F5'</ShortCut>
          <Description>Rerun 'Development'</Description>
          <Count>11</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F5'</ShortCut>
          <Description>Resume Program</Description>
          <Count>30</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Development'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Run 'Development'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F10'</ShortCut>
          <Description>Step Over</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Development'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5'</ShortCut>
          <Description>Stop 'Development'</Description>
          <Count>13</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>